NItext<-read.csv('C:\\Users\\Z\\Desktop\\NI\\Raw_1.csv')
text1<-as.String(NItext$Narrative[4])
aaa<-getMorph(text1,'ALL')######### set morpheme type
tale<-table(aaa)
### +a setting threshold
df1<-data.frame(sort(tale, decreasing = TRUE))
for (i in c(1:length(df1$aaa))){
ifreq<-as.double(df1$Freq[i])
if(ifreq<=2){ ######### set threshold number
next
}
print(as.String(df1$aaa[i]))
}
